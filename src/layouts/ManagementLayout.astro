---
interface Props {
  title: string;
}
import ManagementSidebar from "../components/pages/management/ManagementSidebar.astro";
import Dashboard from "../components/pages/Dashboard.astro";
import Layout from "../layouts/Layout.astro";
const { title } = Astro.props as Props;
---

<Layout title={title}>
  <Dashboard>
    <ManagementSidebar slot="sidebar" />
    <div slot="main-content" class="gap-3 p-3">
      <slot />
    </div>
  </Dashboard>
</Layout>
