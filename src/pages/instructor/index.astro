---
import TeacherLayout from "../../layouts/TeacherLayout.astro";
import Instructor from "../../components/pages/instructor/Instructor.astro";
import type { Course } from "@/components/types/api";
import { TableCourse } from "@/components/services/course/table-course";
import { getCourses } from "@/components/services/course/course";
const courses :Course[] = await getCourses();
---

<TeacherLayout title="teacher">
  <Instructor/>
  <TableCourse client:visible data={courses}/>
</TeacherLayout>
